{
  "name": "@parsekonlepeu/picker",
  "private": false,
  "version": "0.7.1",
  "description": "picker date google like",
  "author": {
    "name": "Nicolas Angeon",
    "email": "parsekonlepeu@gmail.com"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf ./dist",
    "build:esm": "node ../../script/build.mjs esm",
    "build:cjs": "node ../../script/build.mjs cjs",
    "build:types": "tsc -b ./tsconfig.build.json",
    "build": "pnpm clean && pnpm build:cjs && pnpm build:types && pnpm copyFiles",
    "copyFiles": "node ../../script/copyFiles.mjs",
    "publish": "pnpm publish --access public dist"
  },
  "sideEffects": false,
  "dependencies": {
    "@date-io/luxon": "^2.16.1",
    "lodash": "^4.17.21",
    "luxon": "^3.0.3"
  },
  "peerDependencies": {
    "@parsekonlepeu/utils": "workspace:*",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@reduxjs/toolkit": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "core-js": "^3.29.0"
  }
}
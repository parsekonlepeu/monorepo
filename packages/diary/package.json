{
  "name": "@parsekonlepeu/diary",
  "private": false,
  "version": "0.7.1",
  "description": "diary google like",
  "author": {
    "name": "Nicolas Angeon",
    "email": "parsekonlepeu@gmail.com"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf ./dist",
    "build:esm": "node ../../script/build.mjs esm",
    "build:cjs": "node ../../script/build.mjs cjs",
    "build:types": "tsc -b tsconfig.build.json",
    "build": "pnpm clean && pnpm build:cjs && pnpm build:types && pnpm copyFiles",
    "copyFiles": "node ../../script/copyFiles.mjs",
    "publish": "pnpm publish --no-git-checks true --access public dist"
  },
  "sideEffects": false,
  "dependencies": {
    "@date-io/luxon": "^2.16.1",
    "@parsekonlepeu/picker": "workspace:*",
    "@parsekonlepeu/utils": "workspace:*",
    "luxon": "^3.0.3",
    "react-color": "^2.19.3",
    "react-hook-form": "^7.39.7",
    "react-select": "^5.4.0",
    "react-transition-group": "^4.4.5",
    "react-virtualized": "^9.22.3",
    "tinycolor2": "^1.4.2",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.5",
    "@emotion/styled": "^11.10.4",
    "@reduxjs/toolkit": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  }
